@using System.Collections.Generic;
@using Registrar.Models;

@model List<Registrar.Models.Course>

@if (@Model.Count == 0)
{
  <h3>No courses have enrolled yet!</h3>
} 

<ul>
  @foreach (Course course in Model)
  {
      <li>@Html.ActionLink(course.CourseName, "Details", new {id = course.CourseId})</li>
  }   
</ul>

@Html.ActionLink("Add a course", "Create", "Course")
@Html.ActionLink("Back Home", "Index", "Home")